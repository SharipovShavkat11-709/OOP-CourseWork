            <TableColumn fx:id="tableColumnCity" prefWidth="75.0" text="Город" />
            <TableColumn fx:id="tableColumnStreet" prefWidth="75.0" text="Улица" />
            <TableColumn fx:id="tableColumnHouse" prefWidth="75.0" text="Дом" />
        </columns>
