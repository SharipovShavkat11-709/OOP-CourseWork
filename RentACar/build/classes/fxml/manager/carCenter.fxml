<AnchorPane fx:id="anchorPaneBaseDictionary" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.manager.CarCenterController">
